<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>FilterLogIn</filter-name>
        <filter-class>filters.FilterLogIn</filter-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>FilterLogIn</filter-name>
        <servlet-name>Listing</servlet-name>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <filter-mapping>
        <filter-name>FilterLogIn</filter-name>
        <servlet-name>User</servlet-name>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <filter-mapping>
        <filter-name>FilterLogIn</filter-name>
        <servlet-name>Message</servlet-name>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <servlet>
        <servlet-name>Landing</servlet-name>
        <servlet-class>com.cereal.rentingapplicationre.Landing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>com.cereal.rentingapplicationre.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listing</servlet-name>
        <servlet-class>com.cereal.rentingapplicationre.Listing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Message</servlet-name>
        <servlet-class>chat.Message</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Landing</servlet-name>
        <url-pattern>/Landing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listing</servlet-name>
        <url-pattern>/Listing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Message</servlet-name>
        <url-pattern>/Message</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>1440</session-timeout>
    </session-config>
</web-app>
